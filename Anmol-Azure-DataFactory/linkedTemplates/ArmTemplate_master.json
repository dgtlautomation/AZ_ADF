				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'Anmol-Azure-DataFactory_ArmTemplate_2')]"
			]
		}
	]
}
					"LS_properties_typeProperties_userName": {
						"value": "[parameters('LS_properties_typeProperties_userName')]"
					},
					"LS_properties_typeProperties_server": {
						"value": "[parameters('LS_properties_typeProperties_server')]"
					},
					"LS_properties_typeProperties_systemNumber": {
						"value": "[parameters('LS_properties_typeProperties_systemNumber')]"
					},
					"LS_SAP_properties_typeProperties_clientId": {
